        background-image: url('data:image/png;base64,')
        background-color: #bb4733

    &.m_button_action_conference li.g_first:active
        background-image: url('data:image/png;base64,')
        background-color: #c24c39

    &.m_button_action_transfer li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #c24c39

    &.m_button_action_transfer li.g_first:active
        background-image: url('data:image/png;base64,')
        background-color: #c24c39

    &.m_button_action_cancelcall li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #bb4733

    &.m_button_action_cancelcall li.g_first:active
        background-image: url('data:image/png;base64,')
        background-color: #c24c39

    &.m_button_action_starttalk li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #bb4733

    &.m_button_action_starttalk li.g_first:active
        background-image: url('data:image/png;base64,')
        background-color: #c24c39

    &.m_button_action_endcall li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #ce634d

    &.m_button_action_endcall:active li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #ce634d

    &.m_button_action_toggle li.g_first
        background-image: url('data:image/png;base64,')
        background-color: #7cb242

