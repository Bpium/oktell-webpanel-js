@import "if-ie.styl"
@import "functions.styl"
@import "elements.styl"

/* Normalize panel
---------------------------------------------------------------*/
.oktell_panel
    & p,
    & ul,
    & li,
    & table,
    & tbody,
    & tbody,
    & tfoot,
    & thead,
    & tr,
    & th,
    & td
        margin: 0
        padding: 0
        border: 0
        font-weight: 100
        font-style: inherit
        font-size: 100%
        font-family: inherit
        text-align: left
        vertical-align: baseline

    & a img,
    & :link img, & :visited img
        border: 0

    & table
        border-collapse: collapse

    & q:before, & q:after, & blockquote:before, & blockquote:after
        content: ""

    & ul
        list-style: none


/* Main styles
---------------------------------------------------------------*/
.oktell_panel
    width: 280px
    top: 0
    bottom: 0
    position: fixed
    background: transparent
    background-color: #e0e4e7
    background-image: -moz-linear-gradient(90deg, #c7cfd6 0%, #e0e4e7)
    border-left: 1px solid #B4BDBE
    z-index: 202
    line-height: normal
    font-size: 13px

    & *
        font-family: Tahoma, Verdana, Arial, sans-serif
        font-size: 13px
        color: #444

    &.right
        right: -281px

    &.left
        left: -281px

    &.right.dynamic
        right: -281px

    &.left.dynamic
        left: -281px

    & .h_panel_bg
        background: transparent !important
        width: 100%
        height: 100%

    & .h_panel_bg .b_header
        height: 18px
        background-image: url('data:image/png;base64,')
        background-position: 10px center
        background-repeat: no-repeat
        overflow: hidden

    & .h_panel_bg .b_header .b_list_filter
        float: right
        border-bottom: 1px solid #9f9f9f

    & .h_panel_bg .b_header .b_list_filter li
        border-left: 1px solid #9f9f9f
        width: 18px
        height: 18px
        float: left
        background-position: left center
        background-repeat: no-repeat
        cursor: pointer

    & .h_panel_bg .b_header .b_list_filter li:hover
        background-position: -18px center

    & .h_panel_bg .b_header .b_list_filter li:active
        -webkit-box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 0.2)
        box-shadow: inset 0px 0px 10px 0px rgba(0, 0, 0, 0.2)

    & .h_panel_bg .b_header .b_list_filter li.i_online
        background-image: url('data:image/png;base64,')

    & .h_panel_bg .b_header .b_list_filter li.i_group
        background-image: url('data:image/png;base64,')

    & .h_panel_bg .b_header .b_list_filter li.i_online.g_active
        background-color: #cbd4bd
        background-position: -18px center

    & .h_panel_bg .b_header .b_list_filter li.i_group.g_active
        background-color: #eeebd0
        background-position: -18px center

    & .h_panel_bg > .h_padding
        height: 100%

    & .h_panel_bg .h_padding
        padding: 0 !important

    & .i_panel_bookmark
        height: 58px
        position: absolute
        display: inline-block
        top: 18px
        background: #e0e4e7
        border: 1px solid #B4BDBE
        overflow: hidden
        z-index: -1

    &.right .i_panel_bookmark
        left: -40px
        border-right: 0

    &.dynamic .i_panel_bookmark
        visibility: visible

    &.left .i_panel_bookmark
        right: -40px
        border-left: 0

    & .i_panel_bookmark_bg
        background-image: url('data:image/png;base64,')
        background-position: 6px center
        background-repeat: no-repeat
        width: 38px
        height: 60px

    & .jscroll_wrapper
        overflow: visible !important

    & table .b_contact .b_capital_letter
        display: none

    &.noavatar table .b_contact .b_capital_letter
        display: block
        line-height: 24px
        height: 24px
        color: #c4c4c4
        width: 12px !important

        span
            color: #a0a0a0
            font-size: 0.785em

    &.noavatar .b_marks .b_capital_letter
        display: none !important

    &.noavatar .b_marks .b_contact_title
        padding-left: 11px

    &.noavatar table .b_contact .b_contact_avatar
        display: none

    &.webphone.talking:not(.dtmf):not(.conference) .i_conference .b_contact td.b_contact_title span.o_dtmf
        display: inline !important

/* Marks
---------------------------------------------------------------*/
.oktell_panel
    .b_marks
        width: 100%

        p.b_marks_header
            padding: 0 10px
            margin-bottom: -2px

        table
            margin-top: 6px

        .b_marks_time
            float: right
            font-size: 1em

        .b_marks_label
            font-size: 0.85em

        &.h_shadow_top .h_shadow_top,
        &.i_conference .h_shadow_top,
        &.i_extension .h_shadow_top,
        &.i_flash .h_shadow_top
            padding: 6px 0 0 0

        &.i_conference
            background-color: #BAD29B
            overflow: hidden
            display: none
            border-top: 1px solid #869b78

        &.i_conference .b_marks_label
            color: #4b793e

        &.i_extension
            position: relative
            overflow: hidden
            background-color: #9db77b

            & .h_btn-group
                padding: 8px 10px 10px 10px

            & .o_close
                closeCross(0.5, hover = 0)
                top: 3px

            & .o_close:hover
                closeCross(1, hover = 1)

        &.i_extension .b_marks_label
            color: #36622a

        &.i_flash
            background-color: #DAC59A
            border-top: 1px solid #a79f89
            overflow: hidden
            display: none

        &.i_flash .b_marks_label
            color: #a66928

        &.i_phone
            border-top: 1px solid #b1b2b6
            border-bottom: 1px solid #b8b9b9
            background-color: #c5c7cb
            background-image: linear-gradient(top, #bbbcc0 0px, #c5c7cb 6px)
            box-sizing: border-box
            position: relative
            z-index: 2

    .h_shadow_top
        background-image: url('data:image/png;base64,');
        background-position: top left
        background-repeat: repeat-x
        height: 100%
        overflow: hidden

    .b_inconversation table
        width: 100%

    .b_department_header
        border-top: 1px solid #bebebe
        border-bottom: 1px solid #F1F1F1
        background-color: #EEEBD0
        cursor: pointer
        overflow: hidden

        .h_shadow_top
            padding: 3px 10px
            text-overflow: ellipsis
            -o-text-overflow: ellipsis

            span
                color: #8f8f8d
                font-size: 0.875em
                vertical-align: 1px

        &.g_first
            border-top: none




/* Phone number input fields
---------------------------------------------------------------*/
.oktell_panel
    .h_input_padding
        background: white
        border-radius(3px)
        border: 1px solid #949393
        resize: none
        overflow: hidden
        padding: 3px 5px 4px
        background: -webkit-gradient(linear, 0% 0%, 0% 100%, color-stop(1px, #eeeeee), color-stop(5px, #ffffff)) !important
        background: -webkit-linear-gradient(top, #eeeeee 1px, #ffffff 5px) !important
        background: -moz-linear-gradient(top, #eeeeee 1px, #ffffff 5px) !important
        background: -o-linear-gradient(top, #eeeeee 1px, #ffffff 5px) !important
        background: -ms-linear-gradient(top, #eeeeee 1px, #ffffff 5px) !important
        background: linear-gradient(top, #eeeeee 1px, #ffffff 5px) !important
        display: inline-block
        margin: 4px 0

    .h_input_padding input
        width: 100%
        height: 100%
        border: none
        background: none
        resize: none
        margin: 0
        padding: 0
        display: block
        outline: none !important

    .h_input_padding input:focus
        outline: none

    .h_phone_number_input
        padding-left: 36px
        padding-right: 36px
        margin-top: 13px
        margin-bottom: 13px
        position: relative
        background-image: url('data:image/png;base64,')
        background-position: 96% 12px
        background-repeat: no-repeat
        overflow: hidden

    .i_phone_state_bg
        width: 36px
        position: absolute
        left: 0
        background-image: url('data:image/png;base64,')
        background-position: 12px 7px
        background-repeat: no-repeat
        height: 26px

    .b_phone_number_input
        border: 1px solid #949393
        width: 100%
        border-radius(3px)
        line-height: 22px
        height: 22px !important

    .h_phone_number_input  .h_input_padding
        padding-top: 0
        padding-bottom: 0
        height: 22px
        position: relative
        overflow: hidden
        display: block

    .h_phone_number_input .h_input_padding .jInputClear_close
        //margin-right: -20px

    .h_phone_number_bg
        padding: 4px 4px 4px 6px
        z-index: 12
        position: relative

    .i_phone_number_bg_active .i_phone_popup_button
        right: -2px

    .i_phone_popup_button
        right: 0
        top: 0
        bottom: 0
        height: 22px
        position: absolute
        padding: 0 6px
        border-radius(3px, topright)
        border-radius(3px, bottomright)
        background-image: url('data:image/png;base64,')
        background-color: #dddee1
        background-repeat: repeat-x

    .i_phone_popup_button i
        margin-top: 3px
        background-image: url('data:image/png;base64,')
        width: 16px
        height: 16px
        display: inline-block


/* Buttons
---------------------------------------------------------------*/
.oktell_panel
    .btn
        display: inline-block
        padding: 4px 10px 4px
        line-height: 18px
        color: #333333
        text-align: center
        text-shadow: 0 1px 1px rgba(255, 255, 255, 0.75)
        background-color: #fafafa
        background-image: linear-gradient(#ffffff, #ffffff 25%, #e6e6e6)
        background-repeat: no-repeat
        border: 1px solid #ccc
        border-bottom-color: #bbb
        border-radius(4px)
        -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05)
        -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05)
        box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2), 0 1px 2px rgba(0, 0, 0, 0.05)
        cursor: pointer
        *margin-left: .3em
        text-decoration: none

    .btn:first-child
        *margin-left: 0

    .btn:hover
        color: #333333
        text-decoration: none
        background-color: #e6e6e6
        background-position: 0 -15px
        -webkit-transition: background-position 0.1s linear
        -moz-transition: background-position 0.1s linear
        -ms-transition: background-position 0.1s linear
        -o-transition: background-position 0.1s linear
        transition: background-position 0.1s linear

    .btn:focus
        outline: 5px auto -webkit-focus-ring-color
        outline-offset: -2px

    .btn i
        display: inline-block
        width: 16px
        height: 16px
        vertical-align: text-top

    .btn.g_active, & .btn:active
        background-image: none
        -webkit-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05)
        -moz-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05)
        box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05)
        background-color: #d9d9d9 \9
        color: rgba(0, 0, 0, 0.5)
        outline: 0

    .btn.disabled, & .btn[disabled]
        cursor: default
        background-image: none
        background-color: #e6e6e6
        opacity: 0.65
        -webkit-box-shadow: none
        -moz-box-shadow: none
        box-shadow: none

    .btn.m_big
        padding: 7px 12px
        font-size: 17px
        line-height: normal
        border-radius(5px)

    .btn.m_big .icon
        margin-top: 1px


/* Popup phone input panel
---------------------------------------------------------------*/
.oktell_panel
    .b_phone_keypad
        margin: 24px 0 10px
        box-sizing: border-box
        -moz-box-sizing: border-box
        -webkit-box-sizing: border-box
        display: none

    .b_phone_keypad .h_phone_keypad
        border-radius(3px)
        display: inline-block
        border: 1px solid white

    .b_phone_keypad .b_phone_panel
        border: 1px solid #c1c0bf
        border-radius(3px)

    .b_phone_keypad .b_phone_panel li
        border-bottom: 1px solid #c1c0bf
        float: left
        display: inline-block
        border-right: 1px solid #c1c0bf
        border-left: 1px solid white
        box-sizing: border-box
        -moz-box-sizing: border-box
        -webkit-box-sizing: border-box
        width: 64px

    .b_phone_keypad .b_phone_panel li.g_right
        border-right: none

    .b_phone_keypad .b_phone_panel li.g_float_celar
        clear: both

    .b_phone_keypad .b_phone_panel li.g_first
        border-left: none

    .b_phone_keypad .b_phone_panel li button
        border-radius(0 !important)
        border: none
        text-align: center
        line-height: 140%
        font-size: 17px
        display: block
        width: 100%
        height: 100%

    .b_phone_keypad .b_phone_panel li.g_top_left button
        border-radius(3px, topleft)

    .b_phone_keypad .b_phone_panel li.g_top_right button
        border-radius(3px, topright)

    .b_phone_keypad .b_phone_panel li.g_bottom_left button
        border-radius(3px, bottomleft)

    .b_phone_keypad .b_phone_panel li.g_bottom_right button
        border-radius(3px, bottomright)

    .b_phone_keypad .b_phone_panel li button.m_big:hover,
    .b_phone_keypad .b_phone_panel li button.m_big:active
        border: none


/* Resolution styles
---------------------------------------------------------------*/
@media screen and (min-width: 1200px)
    .oktell_panel.right.dynamic
        right: 0 !important

    .oktell_panel.left.dynamic
        left: 0 !important

    .oktell_panel.dynamic .i_panel_bookmark
        visibility: hidden




/* Clear input plugin. It blocks create in javascript.
---------------------------------------------------------------*/
.oktell_panel
    .jInputClear_hover
        width: 100% !important
        height: 22px
        position: relative !important
        background: none !important

    .jInputClear_hover .jInputClear_close
        position: absolute
        right: 8px
        top: 1px
        display: none
        cursor: pointer
        font-size: 1.1em


/* Contact lists
---------------------------------------------------------------*/
.oktell_panel
    table
        width: 100%

    .b_contact
        height: 35px
        display: inline-block !important
        width: 100%
        margin-top: -3px
        margin-bottom: -3px
        clear: left

    .b_contact .selected_text
	    background-color: yellow

    .b_contact td.b_contact_avatar,
    .b_contact td.b_capital_letter
        width: 24px
        padding: 5px 0 5px 12px
        position: relative
        float: left

    .b_contact td.b_contact_action
        width: 60px
        text-align: center
        vertical-align: middle
        color: #828180
        display: none

    .b_contact td.b_contact_title
        font-size: 1.1em
        position: relative
        padding: 9px 26px 5px 7px
        vertical-align: middle
        overflow: hidden
        display: block

    .b_contact td.b_contact_avatar img
        width: 24px
        height: 24px
        float: left
        cursor: pointer
        vertical-align: middle

    .b_contact td.b_contact_avatar.m_default img
        display: none

    .b_contact td.b_contact_avatar.m_default i
        width: 24px
        height: 24px
        float: left
        cursor: pointer
        vertical-align: middle
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    .b_contact td.b_contact_title .wrapword
        overflow: hidden
        float: left
        height: 20px
        position: relative

    .b_contact td.b_contact_title .wrapword *
        white-space: -moz-pre-wrap !important
        white-space: -pre-wrap
        white-space: -o-pre-wrap
        white-space: pre-wrap
        word-wrap: break-word
        word-break: break-all
        white-space: normal

    .b_contact td.b_contact_title .wrapword a
        width: 300%
        display: inline-block

    .b_contact td.b_contact_title a
        color: #24211d
        text-decoration: none

    .b_contact td.b_contact_title b,
    .b_contact td.b_contact_title b span
        font-weight: 700

    .b_contact td.b_contact_title span.o_number
        color: #999
        padding-left: 0px

    .b_contact td.b_contact_title span.o_dtmf
        display: none
        color: #6b844a
        margin-left: 5px
        text-decoration: underline
        cursor: pointer

    .b_contact:hover,
    .b_contact:hover:active,
    .b_contact.m_touch_clicked
        background: rgba( 255, 255, 255, 0.3 )

    .b_contact.m_offline .b_contact_title .wrapword
        opacity: 0.3

    .b_contact.m_offline .b_contact_avatar img
        opacity: 0.5

    .h_main_list
        height: 100%
        overflow: hidden

    .h_main_list .invisibleDep
        margin-bottom: -1px

    .b_main_list
        margin-top: 3px
        margin-bottom: 10px

    .b_main_list .b_contact.m_busy .b_contact_avatar img
        opacity: 0.5

    .b_main_list .b_contact.m_busy .b_contact_avatar .o_busy
        top: 12px
        left: 18px
        position: absolute
        width: 12px
        height: 12px
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    .b_main_list .b_contact.m_busy .b_contact_title *
        color: #BD3B1B



/* Action bar
---------------------------------------------------------------*/
.oktell_button_action
    border: 1px solid #d8dce0
    border-radius(3px)
    height: 26px
    width: 60px
    overflow: hidden
    zoom: 1
    cursor: pointer
    display: inline-block
    visibility: hidden
    white-space: nowrap
    list-style: none
    margin: 0
    padding: 0
    font-weight: 100
    font-style: inherit
    font-size: 100%
    font-family: inherit
    text-align: left
    vertical-align: baseline

    &[class*="m_button_action_"]
        visibility: visible

    li
        height: 24px
        padding: 0 10px
        float: left
        border: 1px solid white
        border-right: 1px solid #d8dce0
        text-align: center
        margin: 0 auto

    li.invisible
        display: none

    li.g_first
        border-radius(3px, topleft)
        border-radius(3px, bottomleft)

    li.g_last
        padding: 0 4px
        border-right: 1px solid white
        border-radius(3px, topright)
        border-radius(3px, bottomright)

    li.g_first.g_last
        border-right: none
        padding: 0 12px

    li img
        padding-top: 3px

.oktell_panel
    .oktell_button_action
        right: 14px
        top: 5px
        visibility: hidden
        position: absolute
        float: right

    .b_contact .oktell_button_action[class*="m_button_action_"]
        visibility: hidden

    .b_contact:hover .oktell_button_action[class*="m_button_action_"],
    .b_contact.m_touch_clicked .oktell_button_action[class*="m_button_action_"]
        visibility: visible

.oktell_button_action.m_static
    position: static !important

.oktell_actions_group_list
    border: 1px solid #d8dce0
    border-radius(3px)
    position: absolute
    margin: 0 auto
    display: none
    z-index: 999
    padding: 0
    font-size: 13px
    font-family: Tahoma

    li
        background: #f7f8f9
        padding: 6px 12px 5px 10px
        border: 1px solid white
        border-bottom-color: #d8dce0
        overflow: hidden
        zoom: 1
        cursor: pointer

    li.g_first
        border-top: 1px solid white !important
        border-radius(3px, top)

    li.g_last
        border-bottom: 1px solid white !important
        border-radius(3px, bottom)

    li:hover
        background: #edeeee

    li span
        line-height: 16px

    li i
        float: left
        margin-right: 6px
        margin-top: 1px
        width: 16px
        height: 16px
        display: inline-block

    li.i_call i,
    li.i_resume i,
    li.i_answer i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_conference i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_intercom i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_endcall i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_transfer i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_toggle i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_ghosthelp i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_ghostlisten i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

    li.i_hold i
        background-image: url('data:image/png;base64,')
        background-repeat: no-repeat

/* Buttons gradient style
---------------------------------------------------------------*/
.oktell_button_action
    li
        background-repeat: repeat-x

    &.m_button_action_call li.g_first,
    &.m_button_action_answer li.g_first,
